{
    "name": "monitorcampanas",
    "version": "1.0.0",
    "productName": "Monitor Campanas",
    "description": "Monitor de campanas",
    "main": "src/main.js",
    "scripts": {
        "start": "electron .",
        "package-win": "electron-packager . MonitorCampanas --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/favi.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Monitor Campanas\"",
        "create-installer-win": "node installers/windows/createinstaller.js",
        "package-linux": "electron-packager . spartacus --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/favi.png --prune=true --out=release-builds",
        "create-debian-installer": "electron-installer-debian --src release-builds/spartacus-linux-x64/ --arch amd64 --config debian.json",
        "create-linux-installer": " electron-installer-redhat --src release-builds/spartacus-linux-x64/ --arch x86_64 --config linux.json"
    },
    "author": "Monitor Spartacus inc",
    "license": "ISC",
    "devDependencies": {
        "electron": "^7.1.2",
        "electron-packager": "^14.0.6",
        "electron-winstaller": "^4.0.0"
    },
    "dependencies": {
        "asterisk-manager": "^0.1.16",
        "bootstrap": "^4.3.1",
        "datatables.net-dt": "^1.10.20",
        "datatables.net-responsive-dt": "^2.2.3",
        "edit-json-file": "^1.3.2",
        "jquery": "^3.4.1",
        "mysql": "^2.17.1",
        "popper.js": "^1.16.0",
        "request": "^2.88.0"
    }
}
